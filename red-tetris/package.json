    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "react-stateless": "^0.2.2",
    "recursive-readdir": "^2.1.0",
    "redial": "^0.5.0",
    "redux": "^3.5.2",
    "redux-actions": "^1.2.0",
    "redux-logger": "^2.6.1",
    "redux-socket.io": "^1.3.1",
    "redux-thunk": "^2.0.1",
    "sass-loader": "^4.1.1",
    "socket.io": "^1.4.6",
    "style-loader": "^0.13.1"
