    "chai-http": "^3.0.0",
    "combokeys": "^2.4.6",
    "cors": "^2.8.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "filter-object": "^2.1.0",
    "fs": "^0.0.1-security",
    "immutable": "^3.8.1",
    "istanbul": "^0.4.5",
    "istanbul-middleware": "^0.2.2",
    "lodash": "^4.17.4",
    "mongoose": "^4.7.7",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "react": "^15.0.2",
    "react-addons-test-utils": "^15.4.2",
